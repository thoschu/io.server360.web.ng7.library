{
  "name": "io.server360.web.ng7.library",
  "version": "1.0.0",
  "description": "Tutorial: Creating a Library with Angular CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thoschu/io.server360.web.ng7.library.git"
  },
  "author": "Tom S. <thoschulte@gmail.com>",
  "bugs": {
    "url": "https://github.com/thoschu/io.server360.web.ng7.library/issues"
  },
  "homepage": "https://github.com/thoschu/io.server360.web.ng7.library#readme",
  "keywords": [
    "Angular",
    "Library",
    "JavaScript",
    "Node.js"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prestart": "npm run build-all",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "_copy-licenses": "concurrently \"copy LICENSE ./projects/ps-image-disclaimer-ng7-lib/\"",
    "_copy-readmes": "concurrently \"copy README.md ./projects/ps-image-disclaimer-ng7-lib/\"",
    "copy-files": "concurrently \"npm run _copy-licenses\" \"npm run _copy-readmes\"",
    "build-libs": "concurrently \"ng build ps-image-disclaimer-ng7-lib\"",
    "build-libs:w": "concurrently \"ng build ps-image-disclaimer-ng7-lib --watch\"",
    "unpublish-pack": "npm unpublish .",
    "deprecate-pack:latest": "npm deprecate ps-image-disclaimer-ng7-lib 'new version'",
    "deprecate-pack:version": "npm deprecate ps-image-disclaimer-ng7-lib@'< x.x.x' 'new version'",
    "del-packs": "concurrently \"cd dist/ps-image-disclaimer-ng7-lib && del '*.tgz' '!-latest.tgz'\"",
    "pack-libs": "concurrently \"cd dist/ps-image-disclaimer-ng7-lib && npm pack\"",
    "copy-npmrc": "concurrently \"copy .npmrc ./dist/ps-image-disclaimer-ng7-lib/\"",
    "publish-packs": "concurrently \"cd ./dist/ps-image-disclaimer-ng7-lib && cat .npmrc && npm publish .\"",
    "publish-packs:x": "cd ./dist/ps-image-disclaimer-ng7-lib && cat .npmrc && npm publish .",
    "build-publish-libs": "npm run copy-files && npm run build-libs && npm run del-packs && npm run pack-libs && npm run copy-npmrc && npm run deprecate-pack:latest && npm run publish-packs",
    "build-publish-libs:dev": "npm run build-libs && npm run del-packs && npm run pack-libs && npm run copy-npmrc && npm run deprecate-pack:latest && npm run publish-packs",
    "build-all": "npm run build-libs && npm run build",
    "app-version": "node -e \"process.stdout.write(require('./package.json').version);\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "core-js": "^2.5.4",
    "ramda": "^0.26.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "copy": "^0.3.2",
    "del-cli": "^1.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
